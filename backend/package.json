{
  "name": "@dob-validator/backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "prebuild": "pnpm install",
    "build": "prisma generate && tsc --skipLibCheck",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "db:setup": "ts-node scripts/setup-database.ts",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:studio:auth": "echo 'Setting up authenticated DB Studio...' && PRISMA_STUDIO_AUTH=true prisma studio --port 5555",
    "db:studio:secure": "node scripts/secure-db-studio.js",
    "db:reset": "prisma migrate reset",
    "db:seed": "ts-node scripts/setup-database.ts",
    "deploy:log": "ts-node scripts/log-deployment.ts",
    "deploy:check": "ts-node scripts/check-deployments.ts",
    "test:e2e": "ts-node scripts/e2e-test.ts",
    "test:integration": "ts-node scripts/integration-test.ts",
    "test:simple": "ts-node scripts/simple-test.ts",
    "test:complete-flow": "ts-node scripts/test-complete-flow.ts",
    "demo": "ts-node scripts/demo-automation.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "@stellar/stellar-sdk": "^13.3.0",
    "@types/nodemailer": "^6.4.17",
    "@types/pdfkit": "^0.13.9",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^3.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nodemailer": "^6.10.1",
    "pdf-lib": "^1.17.1",
    "pdf-thumbnail": "^1.0.6",
    "pdfkit": "^0.15.2",
    "sharp": "^0.34.3",
    "soroban-client": "^1.0.1",
    "stellar-sdk": "^13.3.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^6.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.15.30",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "prisma": "^6.10.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}